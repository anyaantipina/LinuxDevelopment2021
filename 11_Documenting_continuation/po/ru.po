#: ../src/guess_the_number.c:300
#, c-format
msgid "Is the number greater than %s? (yes/no)\n"
msgstr "Число больше %s? (да/нет)\n"

#: ../src/guess_the_number.c:303
msgid "yes\n"
msgstr "да\n"

#: ../src/guess_the_number.c:306
msgid "no\n"
msgstr "нет\n"

#: ../src/guess_the_number.c:309
#, c-format
msgid "I didn't understand your answer \"%s\". Try again\n"
msgstr "Я не понял ваш ответ \"%s\". Попробуйте еще раз\n"

#: ../src/guess_the_number.c:336
#, c-format
msgid "Think a number from %s to %s\n"
msgstr "Загадайте число от %s до %s\n"

#: ../src/guess_the_number.c:338
#, c-format
msgid "I know what number you made up! This is %s\n"
msgstr "Я знаю какое число ты загадал! Это %s\n"
